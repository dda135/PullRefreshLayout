<?xml version="1.0" encoding="utf-8"?>
<manifest package="fanjh.mine.pullrefreshlayout"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ProjectActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".activity.ContentFixedActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".activity.ListViewDemoActivity"></activity>
        <activity android:name=".activity.RecycleViewDemoActivity"></activity>
        <activity android:name=".activity.ScrollViewDemoActivity"></activity>
        <activity android:name=".activity.NestScrollingDemoActivity"></activity>
        <activity android:name=".activity.AutoRefreshOrLoadDemoActivity"></activity>

    </application>


</manifest>